import turtle
wn = turtle.Screen()
wn.bgcolor("lightgreen")
john = turtle.Turtle()
john.pencolor("red")
for i in range(1, 6):
    john.pendown()
    for n in range(1, 5):
        john.pos()
        john.forward(20 *  i)
        john.left(90)
    john.penup()
    john.setpos(-10 * i, -10 * i)
wn.mainloop()
